{
  "name": "@nextnm/nestjs-ses",
  "version": "0.0.2",
  "description": "NestJS provider for sending emails with ses",
  "author": "Nuno Carvalh√£o <nuno.carvalhao@gmail.com>",
  "license": "MIT",
  "main": "dist/index.js",
  "files": [
    "dist/**/*",
    "*.md"
  ],
  "scripts": {
    "start:dev": "tsc -w",
    "build": "tsc",
    "prepare": "npm run build",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "keywords": [
    "nestjs",
    "ses",
    "emails",
    "amazon",
    "aws",
    "email"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NextNC/nestjs-ses"
  },
  "bugs": {
    "url": "https://github.com/NextNC/nestjs-ses"
  },
  "peerDependencies": {
    "@nestjs/common": "^7.0.7"
  },
  "dependencies": {
    "node-ses": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.4"
  },
  "devDependencies": {
    "@nestjs/common": "^7.0.7",
    "@nestjs/core": "^7.0.7",
    "@nestjs/platform-express": "^7.0.7",
    "@nestjs/testing": "7.0.7",
    "@types/express": "4.17.3",
    "@types/jest": "25.1.4",
    "@types/node": "13.9.5",
    "@types/supertest": "2.0.8",
    "jest": "25.2.4",
    "prettier": "2.0.2",
    "supertest": "4.0.2",
    "ts-jest": "25.3.0",
    "ts-node": "8.8.1",
    "tsc-watch": "4.2.3",
    "tsconfig-paths": "3.9.0",
    "tslint": "6.1.0",
    "typescript": "^3.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "homepage": "https://github.com/NextNC/nestjs-ses"
}
